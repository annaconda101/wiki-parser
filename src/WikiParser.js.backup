var WikiParser = function (documentText) {
	this.documentText = documentText;

};

WikiParser.prototype.image = function () {
	var found = this.documentText.match();

	if (found) {
		var source = found[0];
		var caption = found[1];
	}
}; 


WikiParser.prototype.link = function (found) {
  var found = this.documentText.match(/<<([a-zA-Z ]*)\|(\d*)\|([a-zA-Z ]*)/);

  if (found) {
    var article = found[1];
  	var id = found[2];
  	var altHeadline = found[3];

  	return '<' + article + '><' + id + '><' + altHeadline + '>';

	} else {
	
	return null;
	}
};